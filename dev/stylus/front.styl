body
	font: 400 16px Barlow
.set
	display: flex
	flex-direction column
	min-height 100vh
.section
	padding 16px 24px 0
	+desktop()
		padding 16px 40px
		width 100%
.row
	df({
		df:'.row'
	})
	flex-wrap wrap
	+desktop()
		flex-wrap nowrap
.head
	z-index 1000
	.btn
		+desktop()
			padding 8px 16px 9px
	&-row
		padding 4px 0
		align-items: center
		+desktop()
			padding 0
	&-logo
		max-width 142px
		& svg
			height 32px
	&-burger
		w 28px
		&_item,&:after,&:before
			content ''
			wh 28px 2px
			background RGB($pdark_blue)
			display block
		&_item
			margin 6px 0
		+desktop()
			display none
	&-menu
		&-cont
			display none
			+desktop()
				display block
				margin 0 auto
		&_item
			height 40px
			padding 8px 0
			transition .35s ease
			+desktop()
				padding 11px 16px
			a
				color RGB($pdark_blue)
			&.-dropdown
				cursor: pointer
				display flex
				align-items center
				column-gap: 14.35px
			&:hover
				padding 8px 16px
				border-radius 8px
				background-color: RGB($plight_blue,.2)
				+desktop()
					padding: 11px 16px
			&.menu-active
				svg
					transform rotate(180deg)
			span svg
				wh 12px 7px
				transition .35s ease
			position: relative
			&:after
				+desktop()
					wh 100% 40px
					content: ''
					position: absolute
					top: 0

			&:hover
				&>.head-menu_sublist
					opacity: 1
					visibility visible
					display: block
		+desktop()
			display flex
			column-gap 40px
			font 500 16px/19.2px Barlow

		&_sublist
			display: none
			//+desktop()
			w 256px
			position: absolute
			z-index 100
			display: block
			padding 8px
			padding-bottom: 0
			top: 40px
			left: 0
			background: #fff
			box-shadow 0 4px 24px RGBA($plight_blue,.2)
			border 2px solid RGBA($plight_blue,.2)
			border-radius 8px
			opacity: 0
			visibility hidden
			.head-mobile-subitem:hover
				cursor: pointer
				background-color: RGB($plight_blue,.2)
				.head-mobile-subitem_icon use
					fill RGB($pdark_blue)
				span
					color:  RGB($pdark_blue)
			.head-mobile-subitem_icon
				flex 0 0 24px

	&-login
		display none
		+desktop()
			display flex
			column-gap 16px
	&-actions
		margin-left auto
		.-bottom
			position relative
			margin-top 16px
			text-align center
			justify-content center
			svg
				wh 24px
				use
					fill RGB($pdark_gray)
			&:hover
				svg	use
					fill RGB($pdark_blue)
			&:before
				content: ''
				position: absolute
				top: -8px
				left: -8px
				wh calc(100% + (8px * 2)) 2px
				background-color RGBA($plight_blue,.2)
		&-link
			cursor: pointer
			display: flex
			align-items: center
			font-weight: 500
			color RGB($pdark_blue)
			+desktop()
				margin-right: 16px
			&:first-child
				display: none
				+desktop()
					display: block
		&-menu
			position: absolute
			width 100%
			right 24px
			display flex
			top 72px
			justify-content: flex-end;
			column-gap 30px
			+desktop()
				w initial
				position static
				align-items center
				column-gap initial
		.head-menu_sublist
			w 212px
			left initial
			right: 0

		+desktop()
			margin-left 0
		&-user
			wh 25px
			padding: 0!important
			display: none !important
			+desktop()
				wh 40px
				display: flex !important
				align-items: center
				justify-content: center
				svg
					wh 25px
			.head-menu_sublist
				w 256px
	&-mobile
		&-menu
			&-cont
				--header_height: 72px
				wh 100% 100vh //calc( 100vh - var(--header_height) - 20px)
				df({
					df: '.head-mobile-menu-cont'
				})
				flex-direction column
				overflow-y auto
				top 0 //calc( var(--header_height) + 20px)
				position absolute
				padding 96px 24px 0
				left: -100%
				background-color RGB($pwhite)
				z-index 999
				&.-show
					left: 0
				+desktop()
					display: none
				.head-row
					w calc(100% - 48px)
					padding-top 20px
					padding-bottom 20px
					background-color RGB($pwhite)
					position fixed
					top 0
					z-index 1
			display: flex
			flex-direction column
			&.-show
				visibility visible
			&_item
				padding 8px
				margin-bottom 24px
				font 500 16px/19.2px Barlow
				&:hover
					padding 8px
		&-sublist
			display none
			margin-top -16px
			margin-bottom 24px
		&-subitem
			df({
				df: '.head-mobile-subitem'
				ali: center
			})
			border-radius 8px
			padding 8px 16px
			column-gap: 8px
			margin-bottom: 8px
			&:hover
				background-color: RGB($plight_blue,.2)
			&:active
				border-radius 8px
				background-color RGBA($plight_blue,.2)
				.head-mobile-subitem_value
					color RGB($pdark_blue)
			&_icon
				wh 24px
				use
					fill RGB($pdark_gray)
			&_value
				font 500 16px/19.2px Barlow
				color RGB($pdark_gray)


		&-info
			margin-top auto
			margin-bottom 65px
			&_item
				display: flex !important
				margin-bottom: 16px
			.home-row_info
				margin-bottom: 8px
			&-button
				margin-top: 16px
			&-button .btn
				w 100%
				justify-content center
.head,.foot
	position relative
.-bg
	background-image url('../img/login-mobile.jpg')
	background-size cover
	background-position center top
	background-repeat no-repeat
	+tablet()
		background-image url('../img/login.jpg')
.login
	text-align: center
	&-wh
		wh(100vw,90vh)
		display flex
		justify-content center
		+tablet()
			align-items center
		+desktop()
			wh(100vw,100vh)
	&-logo
		wh 215px 48px
		margin-top 64px
		+desktop()
			wh 287px 64px
			margin-top: 0
	&-form
		border-radius 8px
		background RGB($pwhite)
		border 1.5px solid RGBA($plight_blue, .2)
		box-shadow 0 4px 16px RGBA($plight_blue, .16)
		max-width 590px
		margin 80px auto 0
		padding 40px 23px
		&.-with-bg
			w 100%
			+desktop()
				w 590px
				margin 0 0 0 491px
		+desktop()
			margin calc((100vh - 607px) / 2) auto 0
			padding 80px 80px 123px
		.title
			margin-bottom 8px
			font 500 24px/30px Barlow
			+desktop()
				font 500 32px/38.4px Barlow
		.inpt
			margin-bottom 16px
			position relative
			&:last-of-type
				margin-bottom: 0
		.link
			color RGB($ublue)
			+desktop()
				display: inline
				text-align: right
		.subtitle
			margin-bottom 24px
			letter-spacing -.06px
			.link
				display: block
				text-align center
		.btn
			margin-top 24px
			&-eye
				position absolute
				top 16px
				right 16px
		.tar,.tal
			font 400 16px/19.2px Barlow
			color RGB($pdark_gray)
			+desktop()
				//text-align left
				margin-top 16px
		.tar
			margin-top 8px
			text-align: right
		.tal
			margin-top 16px
			text-align center
			+tablet()
				text-align left
		&-actions
			margin-top 16px
			font 400 16px/19.2px Barlow
			color RGB($pdark_gray)
			letter-spacing -.06px
			.link
				color RGB($ublue)
				letter-spacing initial
				&:not(:first-child)
					margin-left 8px
			+desktop()
				font 400 16px/19.2px Barlow
.copy
	text-align: center
	color RGB($pdark_gray)
	+desktop()
		position: fixed
		left: 0
		right: 0
		margin auto
		bottom: 41px
.menu-active
	border-radius 8px
	background-color RGBA($plight_blue,.2)
	.menu_icon
		use
			fill RGB($pdark_blue)
	.menu_value
		color RGB($pdark_blue)
	& + .head-mobile-sublist
		display: block
		opacity: 1
		visibility: visible
.block
	w 100%
	max-width 735px
	margin-top 20px
	margin-bottom 40px
	padding 40px 24px
	background RGB($pwhite)
	border 1.5px solid RGBA($plight_blue,.2)
	box-shadow 0 4px 16px RGBA($plight_blue,.16)
	border-radius 8px
	+desktop()
		w(735px)
		padding 40px
	&.profile-block
		padding 40px 24px
		margin-bottom 80px
		+desktop()
			margin-top 40px
			margin-bottom 40px
	&.-small
		+desktop()
			padding: 24px
	&-title
		font 500 24px/32px Barlow
	&.-pb0
		padding-bottom 12px
	&-btn
		w calc(50% - 4px)
		padding 9px 0 10px
		border-radius 8px
		display: flex
		align-items: center
		column-gap: 11px
		justify-content center
		border 1.5px solid RGBA($plight_blue,.2)
		margin-bottom: 8px
		font 500 16px/19.2px Barlow
		color RGBA($pdark_blue)
		svg
			wh 24px
			use
				fill: RGB($plight_blue)
	.table
		tr
			margin-top 16px
			background RGB($pwhite)
.profile
	&-row
		margin-top 53px
		+desktop()
			margin-top 69px
			padding 0 40px
	&-content
		max-width 1079px
		padding 16px 24px
		+desktop()
			max-width 1226px
			padding 0
	&-aside
		width: 100%
		overflow hidden
		overflow-x auto
		padding: 0 24px
		-ms-overflow-style none;
		scrollbar-width none;
		&::-webkit-scrollbar
			display none
		+desktop()
			padding 0
			margin-right 50px
			max-width 213px
		+desktop-large()
			margin-right 202px
		&-nav
			w fit-content
			overflow-x auto
			display: flex
			background-color RGB($pwhite)
			border 2px solid RGBA($plight_blue,.2)
			padding: 4px
			border-radius 8px
			+desktop()
				display: initial
				background-color: transparent
				border: none
				padding: 0
				w initial
		&-item
			display flex
			align-items center
			padding 11px 15px 10px
			column-gap 16px
			border-radius 8px
			color RGB($pdark_gray)
			font-weight 500
			white-space nowrap
			+desktop()
				margin-bottom 24px
				padding 11px 21px 10px
			svg
				wh(24px)
				use
					fill RGB($pdark_gray)
				display: none
				+desktop()
					display: block
					wh 24px
			&.active,&:hover
				background-color RGBA($plight_blue,.2)
				color RGB($pdark_blue)
				svg use
					fill RGB($pdark_blue)
	&-block
		&-row
			display flex
			flex-wrap wrap
			margin-top 24px
			margin-bottom 16px
			row-gap 16px
			+desktop()
				flex-wrap nowrap
				column-gap 16px
			.inpt
				w 100%
				+desktop()
					w(50%)
	&-table
		margin-top 6px
		+desktop()
			margin-top: 0
		.table
			w(100%)
			td,th
				padding 18px 0 16px
			tr
				border-bottom 1px solid RGB($plight_gray)
			thead
				text-align left
			tbody
				tr
					&:last-child
						+desktop()
							border: 0

		&-surname
			+desktop()
				display none
		td,th
			&:nth-child(2)
				display: none
				+desktop()
					display table-cell
		&.-vis
			td:nth-child(2)
				display: table-cell
		&-details
			margin-top 3px
			+desktop()
				margin-top 40px
			.block-title
				margin-bottom 8px
				font 500 32px/38.4px Barlow
				+desktop()
					margin-bottom 16px
					font-size 24px
					line-height 28.8px
			.table
				margin-bottom: 40px
				td,th
					font 400 16px Barlow
					padding 24px 0
					&:first-child
						w(153px)
						color RGB($pdark_gray)
		&-membership
			w(100%)
			margin-bottom 20px
			+desktop()
				w(398px)
			.table
				margin-bottom 0
			tbody
				tr:last-child
					border-bottom 1px solid RGB($plight_gray) !important
					border-bottom 0 !important
			th,td
				&:last-child
					text-align: right
	&-notify
		&-text
			margin-top 24px
			+desktop()
				width 400px
		&-row
			margin-top 40px
		&-select,&-checkbox
			margin-top 16px
		&-checkbox
			padding 0 4px
			margin-bottom 8px
		&-title
			display flex
			align-items center
			column-gap 16px
			strong
				font 500 16px/19.2px Barlow
			svg
				wh(32px)
				use
					fill RGB($pdark_blue)
			span
				font 400 13px Barlow
				display block
				color RGB($pdark_gray)
		&-checkboxes
			padding-bottom 24px
			margin-bottom 24px
			border-bottom 1.5px solid RGBA($plight_blue,.2)
	&-benefits
		display: flex
		flex-wrap wrap
		+desktop()
			flex-wrap nowrap
		&-text
			margin-top 24px
		&-links
			margin-top 40px
		&-links
			display: none
			flex-direction column
			row-gap 24px
			+desktop()
				display: flex
		&-aside
			+desktop()
				w(250px)
				flex-shrink 0
			+desktop-large()
				w(398px)
		&-link
			font 400 16px/19.2px Barlow
			display: block
			color RGB($pdark_blue)
			text-decoration: underline
			&:hover
				color RGB($plight_blue)
				text-decoration: none
			&:active
				color RGB($pdark_blue)
		&-content
			w 100%
			margin-top 71px
			scroll-behavior smooth
			+desktop()
				height calc(100vh - 386px)
				margin-top 24px
				overflow hidden
				overflow-y scroll
				padding-right 100px
				padding-left 82px
			+desktop-large()
				padding-right 214px
			.block-title,.title
				margin-bottom: 16px
			&-text
				line-height: 19.3px
			&-text,&-list
				margin-bottom 16px
				&+.block-title,&+.title
					margin-top 40px
			&-list
				strong
					font-weight 500
.popup
	wh(100vw,100vh)
	background-color RGB(0,0,0,.5)
	position fixed
	display flex
	align-items center
	justify-content center
	left 0
	top 0
	z-index 1000
	visibility hidden
	opacity 0
	&.-opened
		visibility visible
		opacity 1
	&-inner
		w calc( 100% - 48px)
		position: relative
		max-height 85vh
		overflow: auto
		+desktop()
			w 480px
		&.-large
			w calc( 100% - 48px)
			+desktop()
				w 735px
			.popup-body
				padding 0 24px
				+desktop()
					padding 0 40px
	&-close
		position absolute
		right 8px
		top 8px
		svg
			wh(24px)
			fill RGB($pdark_gray)
		&.-half
			svg
				wh 32px
	&-body
		padding 0 23px
		background-color RGB($pwhite)
		border 1.5px solid RGBA($plight_blue, .2)
		box-shadow 0px 4px 16px RGBA($plight_blue, .16)
		border-radius 8px
	&-half
		padding-top 120px
		display flex
		position fixed
		right 0
		top 0
		opacity 0
		visibility hidden
		background-color #fff
		z-index 999
		transition .35s ease
		+desktop()
			padding-top 0
			background none
		&.-opened
			opacity: 1
			visibility visible
			right: 0
			.popup-half-right
				transform translateX(0%) translateY(0px)
		&-left,&-right
			overflow-y: auto
			wh(100vw, calc(100vh - 120px))
			+desktop()
				wh(50vw,100vh)
				transform translateY(0px)
		&-left
			left: 0
			background-color RGBA(0,0,0,.25)
		&-right
			transition .35s ease
			transform translateX(-50%) translateY(0px)
			background-color #fff
			padding 40px 24px 80px
			+desktop()
				transform translateY(0px)
				padding 201px 0 0 56px
		&-form
			max-width 400px
			margin-top: 40px
			+desktop-large()
				max-width 530px
			.profile-block-row
				margin: 0
			.inpt
				margin-bottom: 16px
				input
					font 400 16px Barlow
					color RGB($pdark_gray)
			.btn
				margin-top: 24px
				+desktop()
					margin-top: 40px
		.popup-close
			transform translate(13px,-51px)
			position: relative
			display: block
			use
				fill RGB($pdark_gray)

.member
	&-popup
		padding 40px 0
		text-align: center
		.block-title
			color: RGB($pdark_blue)
		.btn
			w 100%
			margin-bottom: 8px
			&:last-child
				margin-bottom: 0
			+desktop()
				w initial
				margin-bottom: 0
	&-text
		margin-top 24px
	&-buttons
		margin-top 24px
		display flex
		flex-direction column
		column-gap 8px
		+desktop()
			flex-direction row
			justify-content center
.delete
	&-member
		text-align center
		.member-text
			+desktop()
				width 320px
				margin 8px auto 0
.change
	&-inpt
		position: relative
		&-placeholder
			display flex
			align-items center
			justify-content space-between
		&-popup
			position: relative
			wh 18px
			svg
				wh(18px)
				use
					wh(18px)
					fill RGB($pdark_blue)
			&:hover
				strong
					opacity 1
					visibility visible
			strong
				width 217px
				font 400 13px/15.6px Barlow
				padding 8px 16px
				border-radius 8px
				position absolute
				top -55px
				right -18px
				background-color RGB($pdark_blue)
				color RGB($pwhite)
				opacity: 0
				visibility: hidden
				&:after
					wh(8px)
					transform rotate(45deg)
					position: absolute
					bottom -4px
					right 24px
					content ''
					display block
					background RGB($pdark_blue)
	&-form
		display flex
		flex-direction: column
		row-gap 16px
		margin-top: 24px
		.btn
			w(120px)
	&-row
		column-gap 16px
		row-gap 16px
		color RGB($pdark_gray)
		.inpt
			w(100%)
			+desktop()
				w(50%)
			&.-full
				w(100%)
	&-buttons
		margin-top 8px
		display: flex
		align-items: center
		column-gap: 10px
		flex-wrap wrap
		+desktop()
			margin-top: 24px
			flex-wrap nowrap
		&-update
			+desktop()
				margin-top 40px
		.btn
			w 100%
			margin-bottom 8px
			+desktop()
				w 120px
				margin-bottom: 0
			&:last-child
				margin-bottom 0
			&.-full
				+desktop()
					w 100%
	&-text
		font 400 16px/19.2px Barlow
	&-select
		.select-head
			background-color RGBA(var(--plight_blue), 0.08)
			border: 0
.table
	&-actions
		text-align center
	&-links
		.link
			font-weight: 500

.home
	&-section
		+desktop()
			w calc( 100% - 480px)
			margin 0 240px
			padding: 53px 0 0
	&-row
		w 100%
		align-items: center
		border-bottom 1.5px solid RGBA($plight_blue,.2)
		padding-bottom: 18px
		display: none
		+desktop()
			display: flex
		&-titles
			-webkit-column-gap: 32px
			-moz-column-gap: 32px
			column-gap: 32px
			margin-left: auto
		&_info
			&>span,&>strong
				font 400 16px/19.2px Barlow
			&>span
				color RGB($pdark_gray)
	&-white
		&-items
			+desktop()
				w 1024px
				flex-shrink 0
				display flex
				align-items flex-start
				flex-wrap wrap
				gap 16px
				.white-block
					margin 0
	&-mobile
		&-hidden
			display none
			+desktop()
				display block
.white
	&-items
		margin-top 50px
		row-gap 70px
		+desktop()
			margin-top: 32px
	&-block
		position: relative
		border-radius 8px
		padding 24px
		margin-bottom: 16px
		border 1.5px solid RGBA($plight_blue,.2)
		background-color RGB($pwhite)
		box-shadow 0 4px 16px RGBA($plight_blue,.16)
		+desktop()
			margin-right: 16px
			min-width 504px
			max-width 504px
		&:last-child
			margin-bottom 0
		&-inner
			padding 24px
		&.-upgrade
			padding 0
			&:hover
				.white-upgrade
					opacity: 1
		.btn
			w 100%
			+desktop()
				width 133px
				padding 6px
			&.-bi
				+desktop()
					w 219px
		&.-large
			padding-bottom: 24px
		&_head
			display: flex
			flex-direction column
			+desktop()
				flex-direction row
				align-items: center
				justify-content: space-between
		&-status
			font 700 13px/15.6px Barlow
			color RGB($ugreen)
			text-transform: uppercase
			&.-inactive
				color RGB($plight_ocher)
			svg
				wh 16px
				margin-right: 6px
		&_title
			display flex
			align-items center
